{
  "manifest_version": 2,
  "name": "Zen Tab",
  "version": "0.1.0",
  "description": "Zen Tab is a new tab page that helps you focus on what's important.",
  "permissions": ["tabs", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "Zen Tab"
  }
}
